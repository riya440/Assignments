def lesser_of_two_evens(a,b):
    if(a%2==0 and b%2==0):
        print("both no are even so lesser no is:")
        if a<=b:
            return a
        else:
            return b
    else:
        print("both no are not even so greater no is:")
        if a>=b:
            return a
        else:
            return b

a=int(input("enter the first no:"))
b=int(input("enter the second no:"))
print(lesser_of_two_evens(a,b))
